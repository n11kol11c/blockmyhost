{
    "$build": {
        "@includeCache": true,
        "@includeUptime": true,
        "@includeDirCheching": true,
        "@includeRebuilding": false,
        "@includeLongMode": false
    },

    "$settings": {
        "@enableFullScreen": true,
        "@enableColoring": true,
        "@maxFullScreenSize": "1920^1080",
        "@minFullScreenSize": [null, null],
        "@enableComfirmations": [true, "strict"],
        "@enableAdministratorPermissions": true
    },
}
